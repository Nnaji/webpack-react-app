{
    "name": "webpack-react-app",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "node-sass": "node-sass",
        "rimraf": "rimraf",
        "dev-rimraf": "npx rimraf ./dist",
        "nscss": "npx node-sass ./src/*.scss",
        "webpack": "webpack",
        "webpack-dev": "webpack-dev-server",
        "debug": "node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js",
        "prod": "npm run webpack-dev -- --open --env.mode production --hot --watch",
        "prod-debug": "npm run debug -- --env.mode production",
        "dev-build": "npm run webpack-dev -- --env.mode production --hot",
        "dev": "npm run webpack-dev -- --open --env.mode development --hot --watch",
        "dev-debug": "npm run debug -- --env.mode development",
        "build": "npm run dev-rimraf && npm run webpack -- --env.mode production",
        "mocha": "mocha",
        "test": "npm run mocha -- -w",
        "dev-test": "npm run mocha",
        "start": "npm run dev-rimraf && npm run prod"
    },
    "keywords": [],
