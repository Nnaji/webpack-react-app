        "node-sass": "^4.13.1",
        "prettier": "^1.19.1",
        "rimraf": "^3.0.2",
        "sass-loader": "^8.0.2",
        "style-loader": "^1.1.3",
        "stylelint": "^12.0.1",
        "stylelint-config-prettier": "^8.0.1",
        "stylelint-prettier": "^1.1.2",
        "url-loader": "^3.0.0",
        "webpack": "^4.42.0",
